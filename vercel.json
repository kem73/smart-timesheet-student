{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "dist/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/students", "dest": "/api/students.js" },
    { "src": "/api/schedule", "dest": "/api/schedule.js" },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ]
}
